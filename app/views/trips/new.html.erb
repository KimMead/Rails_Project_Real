<h4>Create A New Trip</h4> 

<%= form_for :trip, url: user_trips_path do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
    <br/>
    <br/>
    <%= f.label :state_id, "State" %>
    <%= f.collection_select :state_id, State.all, :id, :name, prompt: true %>
    <br/>
    <br/>
    <%= f.label :location, "Location" %>
    <%= f.text_field :location %>
    <br/>
    <br/>
    <%= f.submit %>
  <% end %>