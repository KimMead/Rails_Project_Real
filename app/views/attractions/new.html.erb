<h1>Create A New Attraction</h1> 

<%= form_for @attraction do |f| %>
    <%= f.text_field :name, placeholder: "Name" %>
    <br/>
    <br/>
    <%= f.label :state_id, "Select State" %>
    <%= f.collection_select :state_id, State.order('name ASC'), :id, :name, :include_blank => true %> 
    <br/>
    <br/>
    <%= f.submit %>
  <% end %>

  
