<h1>Create A New Attraction</h1> 

<%= form_for @attraction do |f| %>
    <%= f.text_field :name, placeholder: "Name" %>
    <br/>
    <br/>
    <%= f.text_field :state, placeholder: "State" %>
    <br/>
    <%= f.text_field :location, placeholder: "Location" %>
    <br/>
    <br/>
    <%= f.submit %>
  <% end %>

  
