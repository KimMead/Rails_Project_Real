<h1>Create A New Attraction</h1> 

<%= form_for @attraction do |f| %>
    <%= f.text_field :name, placeholder: "Name" %>
    <br/>
    <br/>
    <%= f.label :state, "State" %>
    <%= f.collection_select :state, State.all, :id, :name, prompt: true %>
    <br/>
    <br/>
    <%= f.text_field :location, placeholder: "Location" %>
    <br/>
    <br/>
    <%= f.submit %>
  <% end %>

  
